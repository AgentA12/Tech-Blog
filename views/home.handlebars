<ul>
  {{#each dbPostData}}
    <li>
      <div class="post-container">
        <div class="post-title">
          <h3>{{"posts.post_title"}}</h3>
          <p>Posted by {{username}} on {{createdAt}}</p>
        </div>
        <div class="post-body">
          <p>{{"posts.post_body"}}</p>
        </div>
      </div>
    </li>
  {{/each}}
</ul>